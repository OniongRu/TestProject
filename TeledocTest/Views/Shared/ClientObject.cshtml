@model Client

 <div class="col-lg-4, listClients" onclick="location.href='http://google.com';">
    <h2>[@Model.Id] @Model.Name</h2>
    <p>ИНН: @Model.TIN</p>
    <p>Дата добавления: @Model.AddingDate</p>
    <p>Дата редактирования: @Model.ChangingDate</p>
    <p>Кол-во учредителей: @Model.Founders.Count()</p>
 </div>